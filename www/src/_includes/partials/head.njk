<link rel="shortcut icon" href="/favicon.png" type="image/png">

<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<meta name="generator" content="{{ eleventy.generator }}">
<meta name="description" content="{{ description }}">

{# Opengraph #}
<meta property="og:title" content="{{ title }}">
<meta property="og:type" content="website">
<meta property="og:url" content="{{ domain }}">
<meta property="og:description" content="{{ og.description }}">
<meta property="og:image" content="{{ og.img.src }}">
<meta property="og:image:width" content="1280">
<meta property="og:image:height" content="640">
<meta property="og:image:alt" content="{{ og.img.alt }}">

<title>{{ title }}</title>

<link rel="stylesheet" href="{{ '/scss/main.scss' | url }}">
{# For per-page custom styles #}
{% block styles %}{% endblock %}

{# umami #}
<script async defer data-website-id="07502b1b-fb3d-4a16-ad41-8e36240c31f1" src="https://umami.karawale.in/binoculars.js"></script>